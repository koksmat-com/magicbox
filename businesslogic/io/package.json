{
  "name": "@koksmat/io",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    ".prebuild": "npx rimraf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts ",
    "build:pack": "export debug=powershell* && npx ts-node --project tsconfig-ecma.json ./src/pack",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts ",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "copyfiles": "npx ts-node copyfiles.ts",
    "pack": "npx ts-node --project tsconfig-ecma.json ./src/pack",
    "parsecsv": "npx ts-node --project tsconfig-ecma.json ./src/parsecsv",
    "test-ts": "pwsh -File ./src/connectors/exchange.ps1 && pwsh -File ./src/packs/sharedmailbox/create.mail1.test.ps1 &&  npx ts-node ./src/packs/sharedmailbox/create.ps1.test.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^4.5.0",
    "@koksmat/core": "workspace:*",
    "@koksmat/powerpacks": "workspace:*",
    "@koksmat/schemas": "workspace:*",
    "@types/lodash": "^4.14.191",
    "csv": "^6.2.10",
    "csv-parse": "^5.3.8",
    "debug": "^4.3.4",
    "expree": "^2.1.0",
    "glob": "^9.3.2",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "rimraf": "^4.4.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@koksmat/tsconfig": "workspace:*",
    "@types/jest": "^29.5.1",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "babel-jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-config-acme": "workspace:*",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.1.1",
    "tsup": "^5.12.9",
    "typescript": "^4.9.5"
  },
